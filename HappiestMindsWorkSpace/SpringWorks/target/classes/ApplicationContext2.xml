<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="camera" class="com.hm.autowire.Camera">
		<property name="type" value="digital" />
		<property name="mPixel" value="15"/>
	</bean>

	<bean id="screen" class="com.hm.autowire.Screen">
		<property name="glassType" value="gorilla"/>
		<property name="length" value="4"/>
		<property name="breadth" value="3"/>
	</bean>

	
	<bean id="speaker" class="com.hm.autowire.Speaker">
		<property name="type" value="dolby"/>
		<property name="volLevels" value="10"/>
	</bean>

</beans>