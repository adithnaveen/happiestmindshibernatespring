package com.hm.client;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import com.hm.model.Account;

public class AppClient02 {

	public static void main(String[] args) {
		ApplicationContext context = new ClassPathXmlApplicationContext("AopContext.xml");
		
		
		Account account1 = context.getBean("account1", Account.class);
		
		System.out.println(account1.getAccNo());
		
	}
}
